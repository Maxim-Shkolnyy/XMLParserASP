@model IQueryable<xmlParserASP.Models.MyCategory>

@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html>
<head>
    <title>Оформление заказа</title>
</head>
<body>
<div>
    <table>
        <tr>
            <th>
                Category Id
            </th>
            <th>
                Category Name
            </th>
        </tr>
        @foreach(var cat in Model)
        {
            <tr>
                <td>
                    @cat.CatId
                </td>
                <td>
                    @cat.Cat_Name
                </td>
            </tr>               

        }

    </table>
</div>
