@model IEnumerable<xmlParserASP.Entities.Gamma.ProductsManualSetQuanity>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Sku)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SetInStockQty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateEnd)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Sku)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SetInStockQty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateStart)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateEnd)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Sku">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Sku">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Sku">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
