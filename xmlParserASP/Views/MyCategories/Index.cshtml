@model IEnumerable<xmlParserASP.Entities.MyCategory>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MyParentCatId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MyCatNameRU)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MyCatNameUA)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MyParentCatId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MyCatNameRU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MyCatNameUA)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MyCatId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MyCatId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MyCatId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
